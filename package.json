{
  "name": "vk-auth-prompt",
  "description": "CLI auth prompt for vk.com api - https://vk.com/dev",
  "version": "0.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel -d dist src",
    "build:watch": "babel -w -d dist src",
    "lint": "eslint src/**/*.js",
    "postpublish": "rm -rf dist",
    "prepublish": "npm run build",
    "prepush": "npm run lint && npm test",
    "test": "npm run build && ava --serial",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "vk",
    "auth-prompt"
  ],
  "dependencies": {
    "inquirer-question": "^1.2.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "~6.6.0",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "~6.6.0",
    "configstore": "^2.0.0",
    "eslint": "^2.7.0",
    "husky": "^0.11.3",
    "inquirer-test": "^1.0.5",
    "pify": "^2.3.0",
    "userhome": "^1.0.0"
  },
  "author": "ewnd9 <ewndnine@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/vk-auth-prompt.git"
  }
}
